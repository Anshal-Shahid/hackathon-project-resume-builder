{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.ts",
      "use": "@vercel/static"
    },
    {
      "src": "fonts/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/panel.html",
      "dest": "/panel.html"
    },
    {
      "src": "/panel.css",
      "dest": "/panel.css"
    },
    {
      "src": "/script.js",
      "dest": "/script.js"
    },
    {
      "src": "/script.ts",
      "dest": "/script.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/panel.html"
    },
    {
      "src": "./fonts/(.*)",
      "dest": "/fonts/$1"
    }
  ]
}
